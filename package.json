{
  "name": "uber-clone",
  "version": "1.0.1",
  "description": "",
  "main": "./lib/server.js",
  "scripts": {
    "start": "nodemon ./lib/server.js",
    "test": "nodemon --watch ./src --exec 'npm run prod && mocha --recursive ./lib/test -R min' ",
    "prod": "babel src -d lib",
    "postinstall": "npm run prod",
    "dev": "nodemon --watch ./src --exec 'npm run prod && npm run start' ",
